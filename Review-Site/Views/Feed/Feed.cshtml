@model Review_Site.Data.Models.Feed.Feed
@{
    Layout = null;
}

<?xml version="1.0" encoding="UTF-8" ?>
<rss version="2.0">
<channel>
        <title>@Model.Title</title>
        <description>@Model.Description</description>
        <link>@Model.Link</link>
        <lastBuildDate>@Model.LastBuildDate</lastBuildDate>
        <pubDate>@Model.PubDate</pubDate>
        <ttl>@Model.TTL</ttl>
        @foreach(Review_Site.Data.Models.Feed.FeedItem item in Model.FeedItems){
        <item>
                <title>@item.Title</title>
                <description>@item.Description</description>
                <link>@item.Link</link>
                <guid>@item.Guid</guid>
                <pubDate>@item.PubDate</pubDate>
        </item>
        }
</channel>
</rss>
